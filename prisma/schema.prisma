 

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
   id              String   @id @default(uuid())
   clerkUserId     String   @unique
   email           String   @unique
   name            String?
   imageUrl        String?
   industry        String?
}

 